.global _start

  .section .text

  movq $node_0, %rax
  cmpq $761, (%rax)
  jne bad_exit
  cmpq $node_1, 8(%rax)
  jne bad_exit
  cmpq $0, 16(%rax)
  jne bad_exit

  movq $node_1, %rax
  cmpq $567, (%rax)
  jne bad_exit
  cmpq $node_2, 8(%rax)
  jne bad_exit
  cmpq $new_node, 16(%rax)
  jne bad_exit

  movq $node_2, %rax
  cmpq $358, (%rax)
  jne bad_exit
  cmpq $node_4, 8(%rax)
  jne bad_exit
  cmpq $node_3, 16(%rax)
  jne bad_exit

  movq $node_4, %rax
  cmpq $126, (%rax)
  jne bad_exit
  cmpq $0, 8(%rax)
  jne bad_exit
  cmpq $0, 16(%rax)
  jne bad_exit

  movq $node_3, %rax
  cmpq $438, (%rax)
  jne bad_exit
  cmpq $0, 8(%rax)
  jne bad_exit
  cmpq $0, 16(%rax)
  jne bad_exit

  movq $new_node, %rax
  cmpq $685, (%rax)
  jne bad_exit
  cmpq $0, 8(%rax)
  jne bad_exit
  cmpq $0, 16(%rax)
  jne bad_exit


  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  root: .quad node_0
  node_0:
            .quad 761
            .quad node_1
            .quad 0
  node_1:
            .quad 567
            .quad node_2
            .quad 0
  node_2:
            .quad 358
            .quad node_4
            .quad node_3
  node_4:
            .quad 126
            .quad 0
            .quad 0
  node_3:
            .quad 438
            .quad 0
            .quad 0
  new_node: .quad 685, 0, 0


