.global _start

  .section .text

  movq $head, %rax
  cmpq $node_0, 0(%rax)
  jne bad_exit

  movq $node_0, %rax
  cmpl $426, (%rax)
  jne bad_exit
  cmpq $node_1, 4(%rax)
  jne bad_exit

  movq $node_1, %rax
  cmpl $271, (%rax)
  jne bad_exit
  cmpq $node_3, 4(%rax)
  jne bad_exit

  movq $node_3, %rax
  cmpl $127, (%rax)
  jne bad_exit
  cmpq $node_2, 4(%rax)
  jne bad_exit

  movq $node_2, %rax
  cmpl $579, (%rax)
  jne bad_exit
  cmpq $0, 4(%rax)
  jne bad_exit


  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head:
            .quad node_0
  node_0:
            .int 426
            .quad node_1
  node_1:
            .int 271
            .quad node_2
  node_2:
            .int 579
            .quad node_3
  node_3:
            .int 127
            .quad 0
  Source: .quad node_2
  Value: .int 127


