.global _start

  .section .text

  cmpq $205, (node_0)
  jne bad_exit
  cmpq $606, (node_1)
  jne bad_exit
  cmpq $970, (node_2)
  jne bad_exit
  cmpq $916, (node_3)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 205
          .quad node_1
  node_1:
          .quad 606
          .quad node_2
  node_2:
          .quad 916
          .quad node_3
  node_3:
          .quad 970
          .quad 0
  Source: .quad node_3
  Value: .quad 916


