.global _start

  .section .text

  movq $head, %rax
  cmpq $node_0, 0(%rax)
  jne bad_exit

  movq $node_0, %rax
  cmpq $514, (%rax)
  jne bad_exit
  cmpq $node_1, 8(%rax)
  jne bad_exit

  movq $node_1, %rax
  cmpq $935, (%rax)
  jne bad_exit
  cmpq $node_2, 8(%rax)
  jne bad_exit

  movq $node_2, %rax
  cmpq $709, (%rax)
  jne bad_exit
  cmpq $0, 8(%rax)
  jne bad_exit


  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head:
            .quad node_0
  node_0:
            .quad 514
            .quad node_1
  node_1:
            .quad 935
            .quad node_2
  node_2:
            .quad 709
            .quad 0
  Source: .quad node_0
  val: .int 514


