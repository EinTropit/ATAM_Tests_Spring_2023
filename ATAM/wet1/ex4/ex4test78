.global _start

  .section .text

  cmpq $853, (node_0)
  jne bad_exit
  cmpq $216, (node_1)
  jne bad_exit
  cmpq $62, (node_2)
  jne bad_exit
  cmpq $99, (node_3)
  jne bad_exit
  cmpq $490, (node_4)
  jne bad_exit
  cmpq $969, (node_5)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 853
          .quad node_1
  node_1:
          .quad 216
          .quad node_2
  node_2:
          .quad 62
          .quad node_3
  node_3:
          .quad 99
          .quad node_4
  node_4:
          .quad 490
          .quad node_5
  node_5:
          .quad 969
          .quad 0
  Source: .quad node_2
  Value: .quad 62


