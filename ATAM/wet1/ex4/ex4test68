.global _start

  .section .text

  cmpq $631, (node_0)
  jne bad_exit
  cmpq $611, (node_1)
  jne bad_exit
  cmpq $205, (node_2)
  jne bad_exit
  cmpq $279, (node_3)
  jne bad_exit
  cmpq $58, (node_4)
  jne bad_exit
  cmpq $390, (node_5)
  jne bad_exit
  cmpq $786, (node_6)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 611
          .quad node_1
  node_1:
          .quad 631
          .quad node_2
  node_2:
          .quad 205
          .quad node_3
  node_3:
          .quad 279
          .quad node_4
  node_4:
          .quad 58
          .quad node_5
  node_5:
          .quad 390
          .quad node_6
  node_6:
          .quad 786
          .quad 0
  Source: .quad node_0
  Value: .quad 631


