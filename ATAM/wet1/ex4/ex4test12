.global _start

  .section .text

  cmpq $677, (node_0)
  jne bad_exit
  cmpq $844, (node_1)
  jne bad_exit
  cmpq $469, (node_2)
  jne bad_exit
  cmpq $60, (node_3)
  jne bad_exit
  cmpq $417, (node_4)
  jne bad_exit
  cmpq $513, (node_5)
  jne bad_exit
  cmpq $301, (node_6)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 677
          .quad node_1
  node_1:
          .quad 844
          .quad node_2
  node_2:
          .quad 469
          .quad node_3
  node_3:
          .quad 513
          .quad node_4
  node_4:
          .quad 417
          .quad node_5
  node_5:
          .quad 60
          .quad node_6
  node_6:
          .quad 301
          .quad 0
  Source: .quad node_3
  Value: .quad 60


