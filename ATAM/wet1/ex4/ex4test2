.global _start

  .section .text

  cmpq $619, (node_0)
  jne bad_exit
  cmpq $927, (node_1)
  jne bad_exit
  cmpq $831, (node_2)
  jne bad_exit
  cmpq $600, (node_3)
  jne bad_exit
  cmpq $852, (node_4)
  jne bad_exit
  cmpq $655, (node_5)
  jne bad_exit
  cmpq $477, (node_6)
  jne bad_exit
  cmpq $710, (node_7)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 619
          .quad node_1
  node_1:
          .quad 927
          .quad node_2
  node_2:
          .quad 831
          .quad node_3
  node_3:
          .quad 600
          .quad node_4
