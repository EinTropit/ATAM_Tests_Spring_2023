.global _start

  .section .text

  cmpq $272, (node_0)
  jne bad_exit
  cmpq $486, (node_1)
  jne bad_exit
  cmpq $393, (node_2)
  jne bad_exit
  cmpq $430, (node_3)
  jne bad_exit
  cmpq $14, (node_4)
  jne bad_exit
  cmpq $677, (node_5)
  jne bad_exit
  cmpq $559, (node_6)
  jne bad_exit
  cmpq $371, (node_7)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 272
          .quad node_1
  node_1:
          .quad 486
          .quad node_2
  node_2:
          .quad 393
          .quad node_3
  node_3:
          .quad 371
          .quad node_4
  node_4:
          .quad 14
          .quad node_5
  node_5:
          .quad 677
          .quad node_6
  node_6:
          .quad 559
          .quad node_7
  node_7:
          .quad 430
          .quad 0
  Source: .quad node_7
  Value: .quad 371


