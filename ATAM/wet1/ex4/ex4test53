.global _start

  .section .text

  movq $head, %rax
  cmpq $node_0, 0(%rax)
  jne bad_exit

  movq $node_0, %rax
  cmpl $101, (%rax)
  jne bad_exit
  cmpq $node_1, 4(%rax)
  jne bad_exit

  movq $node_1, %rax
  cmpl $570, (%rax)
  jne bad_exit
  cmpq $node_4, 4(%rax)
  jne bad_exit

  movq $node_4, %rax
  cmpl $877, (%rax)
  jne bad_exit
  cmpq $node_3, 4(%rax)
  jne bad_exit

  movq $node_3, %rax
  cmpl $155, (%rax)
  jne bad_exit
  cmpq $node_2, 4(%rax)
  jne bad_exit

  movq $node_2, %rax
  cmpl $223, (%rax)
  jne bad_exit
  cmpq $node_5, 4(%rax)
  jne bad_exit

  movq $node_5, %rax
  cmpl $873, (%rax)
  jne bad_exit
  cmpq $0, 4(%rax)
  jne bad_exit


  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head:
            .quad node_0
  node_0:
            .int 101
            .quad node_1
  node_1:
            .int 570
            .quad node_2
  node_2:
            .int 223
            .quad node_3
  node_3:
            .int 155
            .quad node_4
  node_4:
            .int 877
            .quad node_5
  node_5:
            .int 873
            .quad 0
  Source: .quad node_4
  Value: .int 223


