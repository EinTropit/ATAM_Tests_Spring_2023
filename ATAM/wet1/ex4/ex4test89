.global _start

  .section .text

  cmpq $70, (node_0)
  jne bad_exit
  cmpq $10, (node_1)
  jne bad_exit
  cmpq $646, (node_2)
  jne bad_exit
  cmpq $545, (node_3)
  jne bad_exit
  cmpq $161, (node_4)
  jne bad_exit
  cmpq $605, (node_5)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 10
          .quad node_1
  node_1:
          .quad 70
          .quad node_2
  node_2:
          .quad 646
          .quad node_3
  node_3:
          .quad 545
          .quad node_4
  node_4:
          .quad 161
          .quad node_5
  node_5:
          .quad 605
          .quad 0
  Source: .quad node_0
  Value: .quad 70


