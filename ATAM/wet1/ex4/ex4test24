.global _start

  .section .text

  cmpq $356, (node_0)
  jne bad_exit
  cmpq $456, (node_1)
  jne bad_exit
  cmpq $712, (node_2)
  jne bad_exit
  cmpq $955, (node_3)
  jne bad_exit
  cmpq $182, (node_4)
  jne bad_exit
  cmpq $473, (node_5)
  jne bad_exit
  cmpq $72, (node_6)
  jne bad_exit
  cmpq $43, (node_7)
  jne bad_exit
  cmpq $88, (node_8)
  jne bad_exit
  cmpq $512, (node_9)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

