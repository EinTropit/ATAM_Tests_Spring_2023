.global _start

  .section .text

  cmpq $303, (node_0)
  jne bad_exit
  cmpq $625, (node_1)
  jne bad_exit
  cmpq $370, (node_2)
  jne bad_exit
  cmpq $132, (node_3)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 303
          .quad node_1
  node_1:
          .quad 132
          .quad node_2
  node_2:
          .quad 370
          .quad node_3
  node_3:
          .quad 625
          .quad 0
  Source: .quad node_1
  Value: .quad 625


