.global _start

  .section .text

  cmpq $700, (node_0)
  jne bad_exit
  cmpq $890, (node_1)
  jne bad_exit
  cmpq $781, (node_2)
  jne bad_exit
  cmpq $763, (node_3)
  jne bad_exit
  cmpq $468, (node_4)
  jne bad_exit
  cmpq $758, (node_5)
  jne bad_exit
  cmpq $138, (node_6)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 700
          .quad node_1
  node_1:
          .quad 890
          .quad node_2
  node_2:
          .quad 781
          .quad node_3
  node_3:
          .quad 763
          .quad node_4
  node_4:
          .quad 468
          .quad node_5
  node_5:
          .quad 758
          .quad node_6
  node_6:
          .quad 138
          .quad 0
  Source: .quad node_2
  Value: .quad 1390


