.global _start

  .section .text

  cmpq $755, (node_0)
  jne bad_exit
  cmpq $846, (node_1)
  jne bad_exit
  cmpq $2, (node_2)
  jne bad_exit
  cmpq $42, (node_3)
  jne bad_exit
  cmpq $299, (node_4)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 755
          .quad node_1
  node_1:
          .quad 846
          .quad node_2
  node_2:
          .quad 2
          .quad node_3
  node_3:
          .quad 299
          .quad node_4
  node_4:
          .quad 42
          .quad 0
  Source: .quad node_4
  Value: .quad 299


