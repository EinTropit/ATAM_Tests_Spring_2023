.global _start

  .section .text

  cmpq $149, (node_0)
  jne bad_exit
  cmpq $480, (node_1)
  jne bad_exit
  cmpq $739, (node_2)
  jne bad_exit
  cmpq $242, (node_3)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 149
          .quad node_1
  node_1:
          .quad 480
          .quad node_2
  node_2:
          .quad 739
          .quad node_3
  node_3:
          .quad 242
          .quad 0
  Source: .quad node_2
  Value: .quad 739


