.global _start

  .section .text

  cmpq $806, (node_0)
  jne bad_exit
  cmpq $797, (node_1)
  jne bad_exit
  cmpq $179, (node_2)
  jne bad_exit
  cmpq $873, (node_3)
  jne bad_exit
  cmpq $701, (node_4)
  jne bad_exit
  cmpq $73, (node_5)
  jne bad_exit
  cmpq $186, (node_6)
  jne bad_exit
  cmpq $295, (node_7)
  jne bad_exit
  cmpq $386, (node_8)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 806
