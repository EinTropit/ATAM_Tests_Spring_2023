.global _start

  .section .text

  cmpq $147, (node_0)
  jne bad_exit
  cmpq $124, (node_1)
  jne bad_exit
  cmpq $16, (node_2)
  jne bad_exit
  cmpq $373, (node_3)
  jne bad_exit
  cmpq $14, (node_4)
  jne bad_exit
  cmpq $560, (node_5)
  jne bad_exit
  cmpq $18, (node_6)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 147
          .quad node_1
  node_1:
          .quad 373
          .quad node_2
  node_2:
          .quad 16
          .quad node_3
  node_3:
          .quad 124
          .quad node_4
  node_4:
          .quad 14
          .quad node_5
  node_5:
          .quad 560
          .quad node_6
  node_6:
          .quad 18
          .quad 0
  Source: .quad node_3
  Value: .quad 373


