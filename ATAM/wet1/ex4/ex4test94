.global _start

  .section .text

  cmpq $115, (node_0)
  jne bad_exit
  cmpq $922, (node_1)
  jne bad_exit
  cmpq $135, (node_2)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 115
          .quad node_1
  node_1:
          .quad 922
          .quad node_2
  node_2:
          .quad 135
          .quad 0
  Source: .quad node_0
  Value: .quad 115


