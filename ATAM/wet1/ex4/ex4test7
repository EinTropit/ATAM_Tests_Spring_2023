.global _start

  .section .text

  cmpq $462, (node_0)
  jne bad_exit
  cmpq $820, (node_1)
  jne bad_exit
  cmpq $230, (node_2)
  jne bad_exit
  cmpq $494, (node_3)
  jne bad_exit
  cmpq $551, (node_4)
  jne bad_exit
  cmpq $301, (node_5)
  jne bad_exit
  cmpq $213, (node_6)
  jne bad_exit
  cmpq $757, (node_7)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 462
          .quad node_1
  node_1:
          .quad 820
          .quad node_2
  node_2:
          .quad 230
          .quad node_3
  node_3:
          .quad 494
          .quad node_4
  node_4:
          .quad 551
          .quad node_5
  node_5:
          .quad 301
          .quad node_6
  node_6:
          .quad 213
          .quad node_7
  node_7:
          .quad 757
          .quad 0
  Source: .quad node_1
  Value: .quad 820


