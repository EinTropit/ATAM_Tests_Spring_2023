.global _start

  .section .text

  cmpq $515, (node_0)
  jne bad_exit
  cmpq $815, (node_1)
  jne bad_exit
  cmpq $496, (node_2)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 515
          .quad node_1
  node_1:
          .quad 815
          .quad node_2
  node_2:
          .quad 496
          .quad 0
  Source: .quad node_2
  Value: .quad 496


