.global _start

  .section .text

  cmpq $473, (node_0)
  jne bad_exit
  cmpq $13, (node_1)
  jne bad_exit
  cmpq $887, (node_2)
  jne bad_exit
  cmpq $466, (node_3)
  jne bad_exit
  cmpq $400, (node_4)
  jne bad_exit
  cmpq $383, (node_5)
  jne bad_exit
  cmpq $892, (node_6)
  jne bad_exit
  cmpq $423, (node_7)
  jne bad_exit
  cmpq $31, (node_8)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 400
          .quad node_1
  node_1:
          .quad 13
          .quad node_2
  node_2:
          .quad 887
          .quad node_3
  node_3:
