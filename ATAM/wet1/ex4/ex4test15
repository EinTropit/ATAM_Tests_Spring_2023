.global _start

  .section .text

  movq $head, %rax
  cmpq $node_1, 0(%rax)
  jne bad_exit

  movq $node_1, %rax
  cmpl $787, (%rax)
  jne bad_exit
  cmpq $node_0, 4(%rax)
  jne bad_exit

  movq $node_0, %rax
  cmpl $564, (%rax)
  jne bad_exit
  cmpq $node_2, 4(%rax)
  jne bad_exit

  movq $node_2, %rax
  cmpl $847, (%rax)
  jne bad_exit
  cmpq $0, 4(%rax)
  jne bad_exit


  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head:
            .quad node_0
  node_0:
            .int 564
            .quad node_1
  node_1:
            .int 787
            .quad node_2
  node_2:
            .int 847
            .quad 0
  Source: .quad node_1
  val: .int 564


