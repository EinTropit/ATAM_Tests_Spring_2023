.global _start

  .section .text

  cmpq $470, (node_0)
  jne bad_exit
  cmpq $46, (node_1)
  jne bad_exit
  cmpq $28, (node_2)
  jne bad_exit
  cmpq $703, (node_3)
  jne bad_exit
  cmpq $414, (node_4)
  jne bad_exit
  cmpq $855, (node_5)
  jne bad_exit
  cmpq $736, (node_6)
  jne bad_exit
  cmpq $113, (node_7)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 470
          .quad node_1
  node_1:
          .quad 46
          .quad node_2
  node_2:
          .quad 28
          .quad node_3
  node_3:
          .quad 703
          .quad node_4
  node_4:
          .quad 855
          .quad node_5
  node_5:
          .quad 414
          .quad node_6
  node_6:
          .quad 736
          .quad node_7
  node_7:
          .quad 113
          .quad 0
  Source: .quad node_4
  Value: .quad 414


