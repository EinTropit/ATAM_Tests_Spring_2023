.global _start

  .section .text

  cmpq $125, (node_0)
  jne bad_exit
  cmpq $543, (node_1)
  jne bad_exit
  cmpq $88, (node_2)
  jne bad_exit
  cmpq $81, (node_3)
  jne bad_exit
  cmpq $385, (node_4)
  jne bad_exit
  cmpq $273, (node_5)
  jne bad_exit
  cmpq $526, (node_6)
  jne bad_exit
  cmpq $619, (node_7)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 125
          .quad node_1
  node_1:
          .quad 543
          .quad node_2
  node_2:
          .quad 88
          .quad node_3
  node_3:
          .quad 81
          .quad node_4
  node_4:
          .quad 385
          .quad node_5
  node_5:
          .quad 526
          .quad node_6
  node_6:
          .quad 273
          .quad node_7
  node_7:
          .quad 619
          .quad 0
  Source: .quad node_5
  Value: .quad 273


