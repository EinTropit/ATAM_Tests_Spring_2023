.global _start

  .section .text

  cmpq $139, (node_0)
  jne bad_exit
  cmpq $172, (node_1)
  jne bad_exit
  cmpq $581, (node_2)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 172
          .quad node_1
  node_1:
          .quad 139
          .quad node_2
  node_2:
          .quad 581
          .quad 0
  Source: .quad node_0
  Value: .quad 139


