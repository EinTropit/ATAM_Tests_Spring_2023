.global _start

  .section .text

  cmpq $41, (node_0)
  jne bad_exit
  cmpq $861, (node_1)
  jne bad_exit
  cmpq $545, (node_2)
  jne bad_exit
  cmpq $952, (node_3)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 41
          .quad node_1
  node_1:
          .quad 861
          .quad node_2
  node_2:
          .quad 952
          .quad node_3
  node_3:
          .quad 545
          .quad 0
  Source: .quad node_3
  Value: .quad 952


