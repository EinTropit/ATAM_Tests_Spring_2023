.global _start

  .section .text

  cmpq $696, (node_0)
  jne bad_exit
  cmpq $581, (node_1)
  jne bad_exit
  cmpq $92, (node_2)
  jne bad_exit
  cmpq $317, (node_3)
  jne bad_exit
  cmpq $250, (node_4)
  jne bad_exit
  cmpq $927, (node_5)
  jne bad_exit
  cmpq $654, (node_6)
  jne bad_exit
  cmpq $261, (node_7)
  jne bad_exit
  cmpq $502, (node_8)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

