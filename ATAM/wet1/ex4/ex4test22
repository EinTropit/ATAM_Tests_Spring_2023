.global _start

  .section .text

  cmpq $663, (node_0)
  jne bad_exit
  cmpq $781, (node_1)
  jne bad_exit
  cmpq $103, (node_2)
  jne bad_exit
  cmpq $69, (node_3)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 663
          .quad node_1
  node_1:
          .quad 781
          .quad node_2
  node_2:
          .quad 103
          .quad node_3
  node_3:
          .quad 69
          .quad 0
  Source: .quad node_2
  Value: .quad 1567


