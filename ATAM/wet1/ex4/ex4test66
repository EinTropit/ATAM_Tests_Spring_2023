.global _start

  .section .text

  cmpq $77, (node_0)
  jne bad_exit
  cmpq $873, (node_1)
  jne bad_exit
  cmpq $596, (node_2)
  jne bad_exit
  cmpq $647, (node_3)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 77
          .quad node_1
  node_1:
          .quad 596
          .quad node_2
  node_2:
          .quad 873
          .quad node_3
  node_3:
          .quad 647
          .quad 0
  Source: .quad node_1
  Value: .quad 873


