.global _start

  .section .text

  cmpq $967, (node_0)
  jne bad_exit
  cmpq $301, (node_1)
  jne bad_exit
  cmpq $394, (node_2)
  jne bad_exit
  cmpq $558, (node_3)
  jne bad_exit
  cmpq $99, (node_4)
  jne bad_exit
  cmpq $146, (node_5)
  jne bad_exit
  cmpq $98, (node_6)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 967
          .quad node_1
  node_1:
          .quad 301
          .quad node_2
  node_2:
          .quad 99
          .quad node_3
  node_3:
          .quad 558
          .quad node_4
  node_4:
          .quad 394
          .quad node_5
  node_5:
          .quad 146
          .quad node_6
  node_6:
          .quad 98
          .quad 0
  Source: .quad node_2
  Value: .quad 394


