.global _start

  .section .text

  cmpq $275, (node_0)
  jne bad_exit
  cmpq $682, (node_1)
  jne bad_exit
  cmpq $942, (node_2)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  head: .quad node_0
  node_0:
          .quad 942
          .quad node_1
  node_1:
          .quad 682
          .quad node_2
  node_2:
          .quad 275
          .quad 0
  Source: .quad node_2
  Value: .quad 942


