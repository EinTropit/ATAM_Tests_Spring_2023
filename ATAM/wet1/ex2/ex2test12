.global _start

  .section .text

  mov $destination, %rax
  cmpb $198, 0(%rax)
  jne bad_exit
  cmpb $216, 1(%rax)
  jne bad_exit
  cmpb $164, 2(%rax)
  jne bad_exit
  cmpb $192, 3(%rax)
  jne bad_exit
  cmpb $196, 4(%rax)
  jne bad_exit
  cmpb $119, 5(%rax)
  jne bad_exit
  cmpb $192, 6(%rax)
  jne bad_exit
  cmpb $165, 7(%rax)
  jne bad_exit
  cmpb $137, 8(%rax)
  jne bad_exit
  cmpb $255, 9(%rax)
  jne bad_exit
  cmpb $70, 10(%rax)
  jne bad_exit
  cmpb $236, 11(%rax)
  jne bad_exit
  cmpb $103, 12(%rax)
  jne bad_exit
  cmpb $168, 13(%rax)
  jne bad_exit
  cmpb $203, 14(%rax)
  jne bad_exit
  cmpb $119, 15(%rax)
  jne bad_exit
  cmpb $85, 16(%rax)
  jne bad_exit
  cmpb $2, 17(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 17
  source: .byte 198,216,164,192,196,119,192,165,137,255,70,236,103,168,203,119,85,0
  destination:
              .zero 17
              .byte 2


