.global _start

  .section .text

  mov $destination, %rax
  cmpb $126, 0(%rax)
  jne bad_exit
  cmpb $132, 1(%rax)
  jne bad_exit
  cmpb $54, 2(%rax)
  jne bad_exit
  cmpb $20, 3(%rax)
  jne bad_exit
  cmpb $47, 4(%rax)
  jne bad_exit
  cmpb $79, 5(%rax)
  jne bad_exit
  cmpb $53, 6(%rax)
  jne bad_exit
  cmpb $165, 7(%rax)
  jne bad_exit
  cmpb $189, 8(%rax)
  jne bad_exit
  cmpb $74, 9(%rax)
  jne bad_exit
  cmpb $10, 10(%rax)
  jne bad_exit
  cmpb $216, 11(%rax)
  jne bad_exit
  cmpb $186, 12(%rax)
  jne bad_exit
  cmpb $94, 13(%rax)
  jne bad_exit
  cmpb $120, 14(%rax)
  jne bad_exit
  cmpb $9, 15(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 15
  source: .byte 126,132,54,20,47,79,53,165,189,74,10,216,186,94,120,0
  destination:
              .zero 15
              .byte 9


