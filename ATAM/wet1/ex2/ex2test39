.global _start

  .section .text

  mov $destination, %rax
  cmpb $141, 0(%rax)
  jne bad_exit
  cmpb $236, 1(%rax)
  jne bad_exit
  cmpb $240, 2(%rax)
  jne bad_exit
  cmpb $155, 3(%rax)
  jne bad_exit
  cmpb $118, 4(%rax)
  jne bad_exit
  cmpb $108, 5(%rax)
  jne bad_exit
  cmpb $94, 6(%rax)
  jne bad_exit
  cmpb $158, 7(%rax)
  jne bad_exit
  cmpb $37, 8(%rax)
  jne bad_exit
  cmpb $230, 9(%rax)
  jne bad_exit
  cmpb $18, 10(%rax)
  jne bad_exit
  cmpb $2, 11(%rax)
  jne bad_exit
  cmpb $197, 12(%rax)
  jne bad_exit
  cmpb $118, 13(%rax)
  jne bad_exit
  cmpb $156, 14(%rax)
  jne bad_exit
  cmpb $211, 15(%rax)
  jne bad_exit
  cmpb $145, 16(%rax)
  jne bad_exit
  cmpb $151, 17(%rax)
  jne bad_exit
  cmpb $245, 18(%rax)
  jne bad_exit
  cmpb $209, 19(%rax)
  jne bad_exit
  cmpb $117, 20(%rax)
  jne bad_exit
  cmpb $193, 21(%rax)
  jne bad_exit
  cmpb $245, 22(%rax)
  jne bad_exit
  cmpb $1, 23(%rax)
  jne bad_exit
  cmpb $8, 24(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 24
  source: .byte 141,236,240,155,118,108,94,158,37,230,18,2,197,118,156,211,145,151,245,209,117,193,245,1,0
  destination:
              .zero 24
              .byte 8


