bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 26
  source: .byte 16,171,64,74,108,159,167,237,230,106,207,38,45,169,44,97,176,140,172,240,203,96,247,28,254,79,0
