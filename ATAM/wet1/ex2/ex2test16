.global _start

  .section .text

  mov $destination, %rax
  cmpb $204, 0(%rax)
  jne bad_exit
  cmpb $194, 1(%rax)
  jne bad_exit
  cmpb $20, 2(%rax)
  jne bad_exit
  cmpb $163, 3(%rax)
  jne bad_exit
  cmpb $239, 4(%rax)
  jne bad_exit
  cmpb $9, 5(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 5
  source: .byte 204,194,20,163,239,0
  destination:
              .zero 5
              .byte 9


