.global _start

  .section .text

  mov $destination, %rax
  cmpb $183, 0(%rax)
  jne bad_exit
  cmpb $226, 1(%rax)
  jne bad_exit
  cmpb $238, 2(%rax)
  jne bad_exit
  cmpb $230, 3(%rax)
  jne bad_exit
  cmpb $125, 4(%rax)
  jne bad_exit
  cmpb $18, 5(%rax)
  jne bad_exit
  cmpb $132, 6(%rax)
  jne bad_exit
  cmpb $253, 7(%rax)
  jne bad_exit
  cmpb $3, 8(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 8
  source: .byte 183,226,238,230,125,18,132,253,0
  destination:
              .zero 8
              .byte 3


