.global _start

  .section .text

  mov $destination, %rax
  cmpb $115, 0(%rax)
  jne bad_exit
  cmpb $55, 1(%rax)
  jne bad_exit
  cmpb $78, 2(%rax)
  jne bad_exit
  cmpb $5, 3(%rax)
  jne bad_exit
  cmpb $0, 4(%rax)
  jne bad_exit
  cmpb $192, 5(%rax)
  jne bad_exit
  cmpb $43, 6(%rax)
  jne bad_exit
  cmpb $128, 7(%rax)
  jne bad_exit
  cmpb $17, 8(%rax)
  jne bad_exit
  cmpb $238, 9(%rax)
  jne bad_exit
  cmpb $82, 10(%rax)
  jne bad_exit
  cmpb $244, 11(%rax)
  jne bad_exit
  cmpb $34, 12(%rax)
  jne bad_exit
  cmpb $117, 13(%rax)
  jne bad_exit
  cmpb $247, 14(%rax)
  jne bad_exit
  cmpb $87, 15(%rax)
  jne bad_exit
  cmpb $216, 16(%rax)
  jne bad_exit
  cmpb $12, 17(%rax)
  jne bad_exit
  cmpb $101, 18(%rax)
  jne bad_exit
  cmpb $67, 19(%rax)
  jne bad_exit
  cmpb $106, 20(%rax)
  jne bad_exit
  cmpb $145, 21(%rax)
  jne bad_exit
  cmpb $27, 22(%rax)
  jne bad_exit
  cmpb $251, 23(%rax)
  jne bad_exit
  cmpb $1, 24(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 24
  source: .byte 115,55,78,5,0,192,43,128,17,238,82,244,34,117,247,87,216,12,101,67,106,145,27,251,0
  destination:
              .zero 24
              .byte 1


