  cmpb $7, 29(%rax)
  jne bad_exit
  cmpb $112, 30(%rax)
  jne bad_exit
  cmpb $1, 31(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 31
  source: .byte 97,120,195,100,235,11,114,212,247,136,27,64,251,6,27,128,81,112,176,141,195,221,146,118,129,206,208,168,144,7,112,0
