.global _start

  .section .text

  mov $destination, %rax
  cmpb $6, 0(%rax)
  jne bad_exit
  cmpb $130, 1(%rax)
  jne bad_exit
  cmpb $244, 2(%rax)
  jne bad_exit
  cmpb $23, 3(%rax)
  jne bad_exit
  cmpb $245, 4(%rax)
  jne bad_exit
  cmpb $73, 5(%rax)
  jne bad_exit
  cmpb $1, 6(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 6
  source: .byte 6,130,244,23,245,73,0
  destination:
              .zero 6
              .byte 1


