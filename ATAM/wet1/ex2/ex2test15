.global _start

  .section .text

  mov $destination, %rax
  cmpb $94, 0(%rax)
  jne bad_exit
  cmpb $35, 1(%rax)
  jne bad_exit
  cmpb $118, 2(%rax)
  jne bad_exit
  cmpb $129, 3(%rax)
  jne bad_exit
  cmpb $72, 4(%rax)
  jne bad_exit
  cmpb $245, 5(%rax)
  jne bad_exit
  cmpb $10, 6(%rax)
  jne bad_exit
  cmpb $114, 7(%rax)
  jne bad_exit
  cmpb $112, 8(%rax)
  jne bad_exit
  cmpb $117, 9(%rax)
  jne bad_exit
  cmpb $188, 10(%rax)
  jne bad_exit
  cmpb $64, 11(%rax)
  jne bad_exit
  cmpb $170, 12(%rax)
  jne bad_exit
  cmpb $252, 13(%rax)
  jne bad_exit
  cmpb $6, 14(%rax)
  jne bad_exit
  cmpb $54, 15(%rax)
  jne bad_exit
  cmpb $197, 16(%rax)
  jne bad_exit
  cmpb $20, 17(%rax)
  jne bad_exit
  cmpb $93, 18(%rax)
  jne bad_exit
  cmpb $37, 19(%rax)
  jne bad_exit
  cmpb $3, 20(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 20
  source: .byte 94,35,118,129,72,245,10,114,112,117,188,64,170,252,6,54,197,20,93,37,0
  destination:
              .zero 20
              .byte 3


