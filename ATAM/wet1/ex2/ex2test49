.global _start

  .section .text

  mov $destination, %rax
  cmpb $128, 0(%rax)
  jne bad_exit
  cmpb $58, 1(%rax)
  jne bad_exit
  cmpb $6, 2(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 2
  source: .byte 128,58,0
  destination:
              .zero 2
              .byte 6


