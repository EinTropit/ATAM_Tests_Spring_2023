.global _start

  .section .text

  mov $destination, %rax
  cmpb $128, 0(%rax)
  jne bad_exit
  cmpb $30, 1(%rax)
  jne bad_exit
  cmpb $21, 2(%rax)
  jne bad_exit
  cmpb $205, 3(%rax)
  jne bad_exit
  cmpb $7, 4(%rax)
  jne bad_exit
  cmpb $27, 5(%rax)
  jne bad_exit
  cmpb $91, 6(%rax)
  jne bad_exit
  cmpb $214, 7(%rax)
  jne bad_exit
  cmpb $47, 8(%rax)
  jne bad_exit
  cmpb $234, 9(%rax)
  jne bad_exit
  cmpb $186, 10(%rax)
  jne bad_exit
  cmpb $164, 11(%rax)
  jne bad_exit
  cmpb $50, 12(%rax)
  jne bad_exit
  cmpb $148, 13(%rax)
  jne bad_exit
  cmpb $216, 14(%rax)
  jne bad_exit
  cmpb $90, 15(%rax)
  jne bad_exit
  cmpb $123, 16(%rax)
  jne bad_exit
  cmpb $178, 17(%rax)
  jne bad_exit
  cmpb $101, 18(%rax)
  jne bad_exit
  cmpb $137, 19(%rax)
  jne bad_exit
  cmpb $98, 20(%rax)
  jne bad_exit
  cmpb $27, 21(%rax)
  jne bad_exit
  cmpb $244, 22(%rax)
  jne bad_exit
  cmpb $144, 23(%rax)
  jne bad_exit
  cmpb $6, 24(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 24
  source: .byte 128,30,21,205,7,27,91,214,47,234,186,164,50,148,216,90,123,178,101,137,98,27,244,144,0
  destination:
              .zero 24
              .byte 6


