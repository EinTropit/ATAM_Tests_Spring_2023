.global _start

  .section .text

  mov $destination, %rax
  cmpb $156, 0(%rax)
  jne bad_exit
  cmpb $82, 1(%rax)
  jne bad_exit
  cmpb $154, 2(%rax)
  jne bad_exit
  cmpb $116, 3(%rax)
  jne bad_exit
  cmpb $203, 4(%rax)
  jne bad_exit
  cmpb $49, 5(%rax)
  jne bad_exit
  cmpb $207, 6(%rax)
  jne bad_exit
  cmpb $21, 7(%rax)
  jne bad_exit
  cmpb $135, 8(%rax)
  jne bad_exit
  cmpb $237, 9(%rax)
  jne bad_exit
  cmpb $198, 10(%rax)
  jne bad_exit
  cmpb $21, 11(%rax)
  jne bad_exit
  cmpb $242, 12(%rax)
  jne bad_exit
  cmpb $49, 13(%rax)
  jne bad_exit
  cmpb $101, 14(%rax)
  jne bad_exit
  cmpb $11, 15(%rax)
  jne bad_exit
  cmpb $32, 16(%rax)
  jne bad_exit
  cmpb $214, 17(%rax)
  jne bad_exit
  cmpb $189, 18(%rax)
  jne bad_exit
  cmpb $40, 19(%rax)
  jne bad_exit
  cmpb $121, 20(%rax)
  jne bad_exit
  cmpb $5, 21(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int 21
  source: .byte 156,82,154,116,203,49,207,21,135,237,198,21,242,49,101,11,32,214,189,40,121,0
  destination:
              .zero 21
              .byte 5


