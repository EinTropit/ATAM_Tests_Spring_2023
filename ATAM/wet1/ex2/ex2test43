.global _start

  .section .text

  mov $destination, %rax
  cmpl $-227, (%rax)
  jne bad_exit
  cmpb $6, 4(%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
  num: .int -227
  source: .byte 0
  destination: .zero 4
               .byte 6


